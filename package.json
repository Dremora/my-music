    "@types/luxon": "^1.15.2",
    "@types/node": "13.1.6",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.0",
    "@types/styled-components": "4.1.18",
    "@typescript-eslint/parser": "2.3.0",
    "apollo": "2.21.2",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-react-app": "5.1.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.0",
    "prettier": "1.19.1",
    "react-scripts": "3.3.0",
    "typescript": "3.7.4"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
    "apollo-link-context": "1.0.18",
    "apollo-link-http": "1.5.15",
    "final-form": "4.18.4",
    "final-form-arrays": "3.0.1",
    "final-form-calculate": "1.3.1",
    "framer-motion": "1.7.0",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "luxon": "1.21.3",
    "normalize.css": "8.0.1",
    "polished": "3.4.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-final-form": "3.6.7",
    "react-final-form-arrays": "1.1.0",
    "react-laag": "1.6.2",
    "react-router-dom": "5.1.2",
    "serve": "11.1.0",
    "styled-components": "4.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext js,ts,tsx",
    "lint:fix": "eslint src --ext js,ts,tsx --fix",
    "now-start": "serve -s build",
    "generate-typescript-graphql-types": "apollo client:codegen --useReadOnlyTypes --globalTypesFile=src/types/graphql.ts --target=typescript types && yarn prettier --write src/**/types/*.ts",
    "deploy": "now && now alias"
  },
  "engines": {
    "node": "12.x"
  },
  "proxy": "http://localhost:4000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
